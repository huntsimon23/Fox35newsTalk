  <div class="container container-fluid">

    <div class="row align-items-center my-5">
      <div class="col-lg-7">
        <img class="img-fluid" src="https://www.broadcastingcable.com/.image/c_limit%2Ccs_srgb%2Cq_auto:good%2Cw_750/MTU0MjAzNDk1NTYyNjgzNzU4/fox-35jpg.webp" alt="">
      </div>

      <div class="col-lg-5">
        <h1 class="font-weight-light">Fox35 NewsTalk!</h1>
        <p>This is a place to discuss all things happening locally in the news!  Click below for the latest headlines and be sure to opine in the comment section under your favorite stories!</p>
        <a class="btn btn-primary" href="#">Get the latest headlines!</a>
      </div>
     
    </div>
  
    <div class="card text-white bg-secondary my-5 py-4 text-center">
      <div class="card-body">
        <p class="text-white m-0">This call to action card is a great place to showcase some important information or display a clever tagline!</p>
      </div>
    </div>

    <div class="row">
      {{#each story}}
      <div class="col-md-4 mb-5">
        <div class="card h-100">
          <div class="card-body">
            <h2 class="card-title">{{this.title}}</h2>
            <p class="card-text"><a href ={{this.link}}>Read Story</a></p>
          </div>
          <div class="card-footer">
            <a href="#" class="btn btn-primary btn-sm">Leave a Comment!</a>
          </div>
        </div>
      </div>
      {{/each}}
